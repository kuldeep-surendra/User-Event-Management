
<body >
<center>
<h1>New event Creation</h1>

<div >

<%= form_for :event,url: user_events_path do |f| %>

<div >
  
<p >
	<%= f.label :title %><br>
	<%= f.text_field :title %> 
	
</p>
<p >
	<%= f.label :venue %><br>
	<%= f.text_field :venue %>

</p>
<p >
	<%= f.label :date %><br>
	<%= f.text_field :date %>

</p>
<p >
	<%= f.label :time %><br>
	<%= f.text_field :time %>


</p>
<p >
	<%= f.label :description %><br>
	<%= f.text_area :description %>


</p>
<p>
	<%= f.submit %>
</p>
</div>
<% end %>
</div>
</center>
</body>